{
  "name": "mtos",
  "version": "0.0.0",
  "description": "package and encrypt content then distribute it over webtorrent and webrtc",
  "main": "index.js",
  "scripts": {
    "test": "standard && node bin/generate-testing-keys.js && tape test/*/*.js | tap-spec",
    "dev": "nodemon --watch '**/*.js' --exec 'tape test/*/*.js | tap-spec'",
    "docs:jsdoc": "jsdoc2md *.js lib/*.js > docs/api.md",
    "docs:jsdoc:watch": "nodemon --ignore _book --exec npm run docs:jsdoc &",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:jsdoc && npm run docs:prepare && gitbook build -g yelouafi/redux-saga",
    "docs:watch": "npm run docs:jsdoc:watch && npm run docs:prepare && gitbook serve"
  },
  "author": "Andrew Davis <diffalot@diff.mx>",
  "license": "LGPL-3.0",
  "dependencies": {
    "diffie-hellman": "^5.0.2",
    "es6-promise": "^4.0.5",
    "events": "^1.1.1",
    "friends-swarm": "https://github.com/diffalot/friends-swarm.git#mtos",
    "get-browser-rtc": "^1.0.2",
    "jszip": "^3.1.3",
    "level-browserify": "^1.1.0",
    "node-forge": "^0.6.45",
    "signalhub": "^4.7.4",
    "subleveldown": "^2.1.0",
    "webrtc-swarm": "^2.8.0",
    "webtorrent": "^0.98.0",
    "wrtc": "0.0.61"
  },
  "devDependencies": {
    "bittorrent-tracker": "^8.0.12",
    "gitbook-cli": "^2.3.0",
    "jsdoc-to-markdown": "^2.0.1",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "globals": {
    },
    "rules": {
      "no-multi-spaces": 0,
      "standard/object-curly-even-spacing": 0
    }
  }
}
